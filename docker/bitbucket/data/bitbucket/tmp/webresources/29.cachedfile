/* progress bar - default is three step */
.progress-bar {
    display: block;
    height: 26px;
    margin: 0 auto 12px;
    padding: 0;
    list-style-type: none;
    width: 480px;
    background: #fff url('/s/364032122/bdc7899/1/6.1.0/_/download/resources/com.atlassian.applinks.applinks-plugin:applinks-css/../../download/resources/com.atlassian.applinks.applinks-plugin:applinks-images/images/config/progress-bar.png') no-repeat 0 0;
